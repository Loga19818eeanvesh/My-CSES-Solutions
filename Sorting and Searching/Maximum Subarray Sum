//  main.cpp
//  Maximum Subarray Sum
//
//  Created by Logas81855anvesh on 01/04/21.
//
 
#include <iostream>
 
using namespace std;
 
int main( ) {
    int n;
    cin>>n;
    long long int p[n];
    long long int r=-10000000000000000;
    for(int i=0;i<n;i++){
        cin>>p[i];
        if(p[i]>r){r=p[i];}
    }
    long long int s=0;
    if(r>=0){
    for(int i=0;i<n;i++){
        s=s+p[i];
        if(s>r){r=s;}
        else if(s<0){s=0;}
    }
    }
    cout<<r<<endl;
    return 0;
    }

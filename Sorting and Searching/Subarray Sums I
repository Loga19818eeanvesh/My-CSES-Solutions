//  main.cpp
//  Subarray Sums I
//
//  Created by Logas81855anvesh on 07/04/21.
//
 
#include <iostream>
 
using namespace std;
 
int main( ) {
    int n;
    cin>>n;
    int x;
    cin>>x;
    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    int p1=0;
    int p2=0;
    int s=a[p1];
    int r=0;
    while(p1<n && p2<n){
        if(s==x){
            if(p1<p2){r++;s=s-a[p1];p1++;}
            else{r++;p2++;s=s+a[p2];}
        }
        else if(s<x){p2++;s=s+a[p2];}
        else if(s>x){
            if(p1<p2){s=s-a[p1];p1++;}
            else{p2++;s=s+a[p2];}
        }
    }
    cout<<r<<"\n";
    return 0;
}

//  main.cpp
//  Nearest Smaller Values
//
//  Created by Logas81855anvesh on 06/04/21.
//
 
#include <iostream>
#include <vector>
 
using namespace std;
struct node{
    int x;
    int i;
};
 
int main( ) {
    int n;
    cin>>n;
    int x[n];
    for(int i=0;i<n;i++){
        cin>>x[i];
    }
    vector<node> v;
    v.push_back({x[0],0});
    cout<<0<<" ";
    for(int i=1;i<n;i++){
        int flag=0;
        for(int j=v.size()-1;j>=0;j--){
            if(v[j].x<x[i]){
                cout<<v[j].i+1<<" ";
                v.push_back({x[i],i});
                flag=1;
            }
            else{
                v.pop_back();
            }
            if(flag==1){break;}
        }
        if(flag==0){
            cout<<0<<" ";
            v.push_back({x[i],i});
        }
    }
    cout<<"\n";
    return 0;
}

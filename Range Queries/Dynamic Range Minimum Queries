//
 
#include <iostream>
#include <vector>
using namespace std;
int min(int a,int b){
    if(a<b){return a;}
    else{return b;}
}
int main( ) {
    vector<int> r;
    int n;
    int q;
    cin>>n;
    cin>>q;
    int x[n];
    for(int i=0;i<n;i++){cin>>x[i];}
    int N;
    if(n==(n&(-n))){
        N=n;
    }
    else{
        if(n%2==0){N=n+2;}
        else{N=n+1;}
    }
    while(N!=(N&(-N))){
        N=N+2;
    }
    int p[2*N];
    p[0]=0;
    for(int i=N;i<2*N;i++){
        if(i-N<n){p[i]=x[i-N];}
        else{p[i]=0;}
    }
    for(int i=N-1;i>0;i--){
        p[i]=min(p[2*i],p[(2*i)+1]);
    }
    int c;
    int a;
    int b;
    for(int i=0;i<q;i++){
        cin>>c;
        cin>>a;
        cin>>b;
        if(c==1){
            x[a-1]=b;
            int i1=N+a-1;
            p[i1]=b;
            i1=i1/2;
            while(i1>0){
                p[i1]=min(p[2*i1],p[(2*i1)+1]);
                i1=i1/2;
            }
        }
        else if(c==2){
            a=a-1+N;
            b=b-1+N;
            int r1=2147483647;
            while(a<=b){
            if(a%2==1){r1=min(r1,p[a]);a++;}
            if(b%2==0){r1=min(r1,p[b]);b--;}
                a=a/2;
                b=b/2;
            }
            r.push_back(r1);
        }
    }
    for(int i=0;i<r.size();i++){
        cout<<r[i]<<endl;
    }
    return 0;
}

#include <iostream>
 
using namespace std;
 
int main( ) {
    int n;
    int q;
    cin>>n;
    cin>>q;
    int p[n];
    for(int i=0;i<n;i++){cin>>p[i];}
    long long int s[n+1];
    s[0]=0;
    for(int i=1;i<=n;i++){
        s[i]=s[i-1]+p[i-1];
    }
    int a;
    int b;
    long long int r[q];
    for(int i=0;i<q;i++){
        cin>>a;
        cin>>b;
        r[i]=s[b]-s[a-1];
    }
    for(int i=0;i<q;i++){cout<<r[i]<<endl;}
    return 0;
}

#include <iostream>
#include <vector>
 
using namespace std;
 
int main( ) {cout<<"\n";
    int n;
    cin>>n;
    vector<string >s(4);
    s[0]="00";
    s[1]="01";
    s[2]="11";
    s[3]="10";
    int m=2;
    while(m<n){
        m=m+1;
        long long int n1=2*s.size();
        s.resize(n1);
        long long int i1=1;
        for(long long int i=n1/2;i<n1;i++){
            s[i]="1"+s[n1/2-i1];
            i1=i1+1;
        }
        for(long long int i=0;i<n1/2;i++){
            s[i]="0"+s[i];
        }
    }
    if(n==1){
        cout<<"0\n"<<"1\n";
    }
    else{
    for(long long int i=0;i<s.size();i++){
        cout<<s[i]<<"\n";
    }
    }
    return 0;
}

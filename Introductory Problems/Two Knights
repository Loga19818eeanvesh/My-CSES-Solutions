#include <iostream>
 
using namespace std;
 
int main( ) {
    long long int n;
    cin>>n;
    long long int l=0;
    cout<<l<<endl;
    long long int r;
    long long int s;
    for(long long int i=2;i<=n;i++){
        s=i*i;
        r=(s*(s-1))/2 - l;
        l=l+8*(i-1);
        cout<<r<<endl;
    }
    return 0;
}

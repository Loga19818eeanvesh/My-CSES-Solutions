
 
#include <iostream>
 
using namespace std;
 
int main( ) {
    int t;
    cin>>t;
    string s[t];
    int a;
    int b;
    for(int i=0;i<t;i++){
        cin>>a;
        cin>>b;
        if( (2*b-a)>=0 && (2*b-a)%3==0 && (2*a-b)>=0 && (2*a-b)%3==0 ){
            s[i]="YES";
        }
        else{
            s[i]="NO";
        }
    }
    for(int i=0;i<t;i++){
        cout<<s[i]<<endl;
    }
    return 0;
}

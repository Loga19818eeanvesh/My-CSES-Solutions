//  main.cpp
//  Trailing Zeros
//
//  Created by Logas81855anvesh on 30/03/21.
//
 
#include <iostream>
 
using namespace std;
 
int main( ) {
    int n;
    cin>>n;
    int r=0;
    int d1=10;
    int r2;
    while(d1<=n){
        r2=n/d1;
        r=r+r2;
        d1=d1*10;
    }
    int d=5;
    int r1;
    int m=2;
    while(d<=n){
        r1=n/d;
        if(r1%m==1){r1=r1-(r1/m);}
        else{r1=r1-(r1/m);}
        m=m*2;
        r=r+r1;
        d=d*5;
    }
    cout<<r<<endl;
    return 0;
}

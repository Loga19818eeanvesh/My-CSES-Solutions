
 
#include <iostream>
 
using namespace std;
 
int main( ) {cout<<endl;
    int n;
    cin>>n;
    if(n==1){cout<<"1"<<endl;}
    else if(n<=3){
        cout<<"NO SOLUTION"<<endl;
    }
    else if(n==4){
        cout<<"2 4 1 3"<<endl;
    }
    else{
        int r=1;
        while(r<=n){
            cout<<r<<" ";
            r=r+2;
        }
        r=2;
        while(r<=n){
            cout<<r<<" ";
            r=r+2;
        }
        cout<<endl;
    }
    return 0;
}

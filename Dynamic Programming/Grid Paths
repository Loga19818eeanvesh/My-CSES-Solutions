#include <iostream>
#include <string>
using namespace std;
int path(string *s,int n,int i,int j,int* d){
    if(i==n-1 && j==n-1){return 1;}
    if(*(d+(i*n)+j)!=-1){return *(d+(i*n)+j);}
    int r=0;
    if(i+1<n && s[i+1][j]=='.'){
        if(*(d+((i+1)*n)+j)==-1){*(d+((i+1)*n)+j)=path(s, n, i+1, j, d)%1000000007;}
        r=r+*(d+((i+1)*n)+j);
        r=r%1000000007;
    }
    if(j+1<n && s[i][j+1]=='.'){
        if(*(d+(i*n)+j+1)==-1){*(d+(i*n)+j+1)=path(s, n, i, j+1, d)%1000000007;}
        r=r+*(d+(i*n)+j+1);
        r=r%1000000007;
    }
    return r;
}
int main( ) {
    int n;
    cin>>n;
    string s[n];
    for(int i=0;i<n;i++){
        cin>>s[i];
    }
    int d[n][n];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            d[i][j]=-1;
        }
    }
    if(s[0][0]=='*' || s[n-1][n-1]=='*'){
        cout<<"0"<<endl;
    }
    else{
    int r=path(s, n, 0, 0, d[0]);
    cout<<r<<endl;
    }
    return 0;
}

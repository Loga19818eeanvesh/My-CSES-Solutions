#include <iostream>
using namespace std;
int main() {
    int n;
    cin>>n;
    int r=0;
    if(n==1000000){n=n-1;r++;}
    while(n>0){
        int a1;
        int a2;
        int a3;
        int a4;
        int a5;
        int a6;
        int maxa=0;
        a1=n%10;
        a2=(n%100)-a1;
        a3=(n%1000)-a2-a1;
        a4=(n%10000)-a3-a2-a1;
        a5=(n%100000)-a4-a3-a2-a1;
        a6=(n%1000000)-a5-a4-a3-a2-a1;
        if(a1>maxa){maxa=a1;}
        if(a2==10){a2=1;}
        else if(a2==20){a2=2;}
        else if(a2==30){a2=3;}
        else if(a2==40){a2=4;}
        else if(a2==50){a2=5;}
        else if(a2==60){a2=6;}
        else if(a2==70){a2=7;}
        else if(a2==80){a2=8;}
        else if(a2==90){a2=9;}
        if(a2>maxa){maxa=a2;}
        if(a3==100){a3=1;}
        else if(a3==200){a3=2;}
        else if(a3==300){a3=3;}
        else if(a3==400){a3=4;}
        else if(a3==500){a3=5;}
        else if(a3==600){a3=6;}
        else if(a3==700){a3=7;}
        else if(a3==800){a3=8;}
        else if(a3==900){a3=9;}
        if(a3>maxa){maxa=a3;}
        if(a4==1000){a4=1;}
        else if(a4==2000){a4=2;}
        else if(a4==3000){a4=3;}
        else if(a4==4000){a4=4;}
        else if(a4==5000){a4=5;}
        else if(a4==6000){a4=6;}
        else if(a4==7000){a4=7;}
        else if(a4==8000){a4=8;}
        else if(a4==9000){a4=9;}
        if(a4>maxa){maxa=a4;}
        if(a5==10000){a5=1;}
        else if(a5==20000){a5=2;}
        else if(a5==30000){a5=3;}
        else if(a5==40000){a5=4;}
        else if(a5==50000){a5=5;}
        else if(a5==60000){a5=6;}
        else if(a5==70000){a5=7;}
        else if(a5==80000){a5=8;}
        else if(a5==90000){a5=9;}
        if(a5>maxa){maxa=a5;}
        if(a6==100000){a6=1;}
        else if(a6==200000){a6=2;}
        else if(a6==300000){a6=3;}
        else if(a6==400000){a6=4;}
        else if(a6==500000){a6=5;}
        else if(a6==600000){a6=6;}
        else if(a6==700000){a6=7;}
        else if(a6==800000){a6=8;}
        else if(a6==900000){a6=9;}
        if(a6>maxa){maxa=a6;}
        n=n-maxa;
        r++;
    }
    cout<<r<<endl;
    return 0;
}

#include <iostream>
using namespace std;
int min_coins(int* c,int n,int v,int* d,int minc){
    if(v==0){return 0;}
    if(v<minc){return 2147483647;}
    if(d[v]!=-1){return d[v];}
    int r=2147483647;
    int r1;
    for(int i=0;i<n;i++){
        if(c[i]<=v){
            if(d[v-c[i]]==-1){d[v-c[i]]=min_coins(c, n, v-c[i], d, minc);}
            if(d[v-c[i]]==2147483647){r1=2147483647;}
            else{r1=1+d[v-c[i]];}
            if(r1<r){r=r1;}
        }
    }
    return r;
}
int main() {
    int n;
    int v;
    cin>>n;
    cin>>v;
    int c[n];
    int minc=1000000;
    for(int i=0;i<n;i++){
        cin>>c[i];
        if(c[i]<minc){minc=c[i];}
    }
    int d[v+1];
    for(int i=0;i<=v;i++){
        d[i]=-1;
    }
    int r=min_coins(c,n,v,d,minc);
    if(r==2147483647){cout<<"-1"<<endl;}
    else{cout<<r<<endl;}
    return 0;
}
